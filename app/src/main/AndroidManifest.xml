<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="be.appfoundry.aipdemo">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="be.appfoundry.aipdemo.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="be.appfoundry.aipdemo.activity.dagger.DaggerActivity" android:label="Dagger">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="be.appfoundry.aipdemo.activity.picasso.WithoutPicassoActivity" android:label="Without Picasso">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="be.appfoundry.aipdemo.activity.picasso.PicassoActivity" android:label="Picasso">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="be.appfoundry.aipdemo.activity.retrofit.RetrofitActivity" android:label="Retrofit">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="be.appfoundry.aipdemo.activity.rxjava.ReactiveActivity" android:label="RXJava">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="be.appfoundry.aipdemo.activity.mvvm.MvvmActivity" android:label="MVVM">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

    </application>

</manifest>